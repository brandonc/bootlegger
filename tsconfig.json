{
  "compilerOptions": {
    "module": "commonjs",
    "sourceMap": true,
    "incremental": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "keyofStringsOnly": true,
    "noEmit": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
